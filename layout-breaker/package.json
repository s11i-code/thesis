{
  "name": "layout-breaker",
  "version": "1.0.0",
  "description": "generate images from broken layouts for fun and science",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint 'src/index.ts' --fix",
    "scrape:debug": "DEBUG=true npm run scrape ",
    "scrape:clear": "rm -rf tmp && npm run scrape",
    "scrape": "npm run compile && node build/index.js  --debug=$DEBUG --site=$SITE --folder=$FOLDER --contindex=$CONTINDEX",
    "sync": "aws s3 sync ./tmp/ s3://layout-bug-detection-dataset  --profile satu-personal-cli --acl public-read",
    "scrape-sync": "npm run scrape && npm run sync",
    "compile": "tsc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.11.7",
    "@types/puppeteer": "^1.20.2",
    "minimist": "^1.2.5",
    "puppeteer": "^5.1.0",
    "puppeteer-cluster": "^0.22.0",
    "random-hash": "^4.0.1",
    "random-sentence": "^1.0.4",
    "random-words": "^1.1.1",
    "shortid": "^2.2.15",
    "string-hash": "^1.1.3",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.1",
    "typescript": "^3.6.4"
  }
}
